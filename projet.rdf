	<Cellule rdf:ID = "caseDepart">
     <rdfs:label xml:lang="fr">0x0</rdfs:label>
     <rdfs:comment>Le joueur démarre le jeu dans cette case</rdfs:comment>
    </Cellule>
	<Cellule rdf:ID = "caseDArrivee">
     <rdfs:label xml:lang="fr">Fin du jeu</rdfs:label>
     <rdfs:comment>Le joueur atteint cette case en cas de victoire</rdfs:comment>
    </Cellule>
	<Cellule rdf:ID = "piege">
     <rdfs:label xml:lang="fr">Piege</rdfs:label>
     <rdfs:comment>Le joueur sera penalisé s'il atteint cette case</rdfs:comment>
